data <- read.csv("BranchSegments.csv", sep=',', head=T)
to_plot <- data[data$stem_m > 0,]
branch <- read.csv("BranchSegments.csv", sep = ',', header = T)
spp <- branch[branch$species=="cherry",]
masses <- spp[spp$stem_m > 0,]
diameter_masses <- lm(log(masses$stem_m)~log(masses$diameter_mm))
diamerer_mass <- list()
ind <- masses[masses$tree==1,]
par(ps = 28, pch= 19)
plot(log(ind$diameter_mm), log(ind$rank), cex = 2, ylim = c(2,8), xlim = c(2,6),
col = "orange", ylab = "log ( Rank )", xlab = "log ( Diameter )",
main = round(summary(diameter_masses)$r.squared, digits = 3), ps = 28, pch= 19)
abline(summary(diameter_masses)$coef[1,1], summary(diameter_masses)$coef[2,1], lwd = 3, lty = 3)
cherry_trees <- c(7,10,15)
colors <- c("red", "darkred", "maroon")
for (j in 1:3){
ind <- spp[spp$tree==cherry_trees[j],]
diameter_mass[[j]] <- lm(log(branch$rank)~log(branch$diameter_mm))
par(new = T)
points(log(ind$diameter_mm), log(ind$rank), cex = 2, col = colors[j])
}
